<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="AddressExecuter" />
		<Option pch_mode="2" />
		<Option compiler="armelfgcc" />
		<Build>
			<Target title="NSG">
				<Option output="../../elfs/$(PROJECT_NAME)/$(PROJECT_NAME)_$(TARGET_NAME).elf" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../Objects/$(PROJECT_NAME)/$(TARGET_NAME)/" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Compiler>
					<Add option="-DNEWSGOLD" />
				</Compiler>
			</Target>
			<Target title="SG">
				<Option output="../../elfs/$(PROJECT_NAME)/$(PROJECT_NAME)_$(TARGET_NAME).elf" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../Objects/$(PROJECT_NAME)/$(TARGET_NAME)/" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Compiler>
					<Add option="-DSGOLD" />
				</Compiler>
			</Target>
			<Target title="ELKA">
				<Option output="../../elfs/$(PROJECT_NAME)/$(PROJECT_NAME)_$(TARGET_NAME).elf" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../Objects/$(PROJECT_NAME)/$(TARGET_NAME)/" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Compiler>
					<Add option="-DNEWSGOLD" />
					<Add option="-DELKA" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-O2" />
			<Add option="-W" />
			<Add option="-std=gnu99" />
			<Add option="-D__NO_LIBC" />
			<Add directory="include" />
		</Compiler>
		<Linker>
			<Add option="-s" />
			<Add option="--defsym __ex=0" />
			<Add option="--gc-section" />
			<Add library="crt" />
			<Add library="crt_helper" />
		</Linker>
		<Unit filename="conf_loader.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="config_data.c">
			<Option compilerVar="CC" />
			<Option compiler="armelfgcc" use="1" buildCommand="$compiler $options -xc $includes -c $file -o $object -O0" />
		</Unit>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
