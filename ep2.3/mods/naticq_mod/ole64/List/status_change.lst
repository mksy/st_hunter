     46          #pragma inline
     47          static void Change_Status(int status)
     48          {
     49            CurrentStatus=status;
     50            set_my_status();
     51          #ifdef NEWSGOLD
     52            SetIconBarHandler();
     53          #endif
     54             extern int isAutoStatusOn;    
     55             extern int msg_away_timer;
     56             extern void away_timer();
     57             extern int away_m;
     58             extern unsigned int AUTO_ENGADE;
     59          
     60            if(isAutoStatusOn!=12 && isAutoStatusOn==status)
     61            {
     62              msg_away_timer=AUTO_ENGADE;
     63              away_m=1;
     64              away_timer();
     65            }
     66            else if(status==IS_ONLINE || status==IS_FFC || status== IS_HOME)
     67            {
     68              msg_away_timer=0;
     69              away_m=0;
     70            }
     71            
     72            SUBPROC((void*)WriteDefSettings);
     73          }
